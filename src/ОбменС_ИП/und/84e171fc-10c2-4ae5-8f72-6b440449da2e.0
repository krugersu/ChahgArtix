		Отбор.ПравилоОбмена.Установить(тССылка.ПравилоОбмена);
		
		НаборЗаписей.Прочитать();
		Если НаборЗаписей.Количество() > 0 Тогда
			МассивДанных.Добавить(НаборЗаписей);
